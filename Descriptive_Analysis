######Min, 1st Qu., Median, Mean, 3rd Qu., Max (all variables)#####
summary(DissectionsK) 

# Calculating average using mean() 
FLengthmean <- mean(DissectionsK$FL_cm, na.rm = TRUE) #124.458
TLengthmean <- mean(DissectionsK$TL_cm, na.rm = TRUE) #112.445
Acantmean <- mean(DissectionsK$Acanthocephalan, na.rm = TRUE) #1.029
Weightmean <- mean(DissectionsK$BodyWWt_g, na.rm = TRUE) #146.564
ConditionF <- mean(DissectionsK$K, na.rm = TRUE) #1.214

#Standard Deviation - All Fish
FLengthSD <- sd(DissectionsK$FL_cm, na.rm = TRUE) #110.982
TLengthSD <- sd(DissectionsK$TL_cm, na.rm = TRUE) #11.860
AcantSD <-sd(DissectionsK$Acanthocephalan, na.rm = TRUE) #3.567
BweightSD <- sd(DissectionsK$BodyWWt_g, na.rm = TRUE) #478.337
ConditionSD <- sd(DissectionsK$K, na.rm = TRUE) #0.129


#Variance - All Fish
FishV <- var(DissectionsK$FL_cm, na.rm = TRUE) #14897.13
TailV <- var(DissectionsK$TL_mm, na.rm = TRUE) #17008.23
BodyV <- var(DissectionsK$BodyWWt_g, na.rm = TRUE) #279635.1
AcantV <- var(DissectionsK$Acanthocephalan, na.rm = TRUE) #4.250758

#Table 1: 
#A table that summarizes one variable and gives the mean and standard deviation for the value of that variable
#Tributary and Acanthocephalan Abundance 
group_by(DissectionsK, TributaryLocation) %>%  
  summarise(                       #Being summarized by mean and sd.
    mean = mean(Acanthocephalan) ,
    sd = sd(Acanthocephalan, na.rm = TRUE))

#Season and Acanthocephalan Abundance 
group_by(DissectionsK, Season) %>%  
  summarise(                       #Being summarized by mean and sd.
    mean = mean(Acanthocephalan) ,
    sd = sd(Acanthocephalan, na.rm = TRUE))
