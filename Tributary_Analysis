#Choptank subset
ChoptankK <- subset(DissectionsK, Tributary_Main == "Choptank")
View(ChoptankK)
summary(ChoptankK)

ChopNoK <- sd(ChoptankK$NonParasitizedK, na.rm = TRUE) 
ChopYesK <- sd(ChoptankK$ParasiztiedK, na.rm = TRUE) 
ChopKSD <- sd(ChoptankK$K, na.rm = TRUE) 

#James Subset 
JamesK <- subset(DissectionsK, Tributary_Main == "James")
View(JamesK)
summary(JamesK)

